/* Barras de Rolagem Customizadas (para overflow-auto) */
.custom-scroll::-webkit-scrollbar {
  width: 8px;
  height: 8px;
}
.custom-scroll::-webkit-scrollbar-track {
  background: #495057;
  border-radius: 10px;
}
.custom-scroll::-webkit-scrollbar-thumb {
  background: #6c757d;
  border-radius: 10px;
}
.custom-scroll::-webkit-scrollbar-thumb:hover {
  background: #868e96;
}

/* Remover setinhas de input type="number" */
input[type="number"]::-webkit-outer-spin-button,
input[type="number"]::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}
input[type="number"] {
  -moz-appearance: textfield;
}

/* Força os componentes .card a ocupar 100% da altura da coluna pai, permitindo scroll interno */
.column > .card {
    height: 100%;
    display: flex;
    flex-direction: column;
}
/* Faz o card-body preencher o espaço restante e permitir scroll */
.column > .card > .card-body.custom-scroll {
    flex-grow: 1;
    overflow-y: auto;
    padding-right: 15px;
    padding-left: 15px;
}
/* Força o padding para card-body que não tem custom-scroll (se houver) */
.column > .card > .card-body:not(.custom-scroll) {
    padding: 1rem !important;
}

/* Refinamentos para os componentes individuais para caberem bem */

