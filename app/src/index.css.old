input[type="number"]::-webkit-outer-spin-button,
input[type="number"]::-webkit-inner-spin-button {
  -webkit-appearance: none; /* Remove a aparência padrão */
  margin: 0; /* Remove a margem que as setinhas poderiam ocupar */
}

/* Para Firefox */
input[type="number"] {
  -moz-appearance: textfield; /* Faz o input number se comportar como um campo de texto normal */
}


/* src/index.css (ou seu arquivo CSS principal) */

/* Garante que o corpo da página e o html também permitam o layout de 100vh */
html, body, #root, .App {
    height: 100%;
    margin: 0;
    padding: 0;
    
    display: flex;
    flex-direction: column;
}

/* Novo wrapper para o Layout componente, que ocupa toda a altura da viewport */
.character-sheet-layout-wrapper {
    height: 100vh;
    display: flex;
    flex-direction: column;
    
}

/* Container principal da ficha de personagem (envolve as 3 colunas) */
.layout-container {
    flex-grow: 1; /* Ocupa todo o espaço vertical restante */
    display: flex;
    width: 100%;
    padding: 15px;
    
    border-radius: 8px;
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.5);
    color: #f8f9fa; /* Cor do texto padrão */
    gap: 15px; /* Espaçamento entre as colunas */
}

/* Estilos para as colunas individuais (.column do Layout.tsx) */
.column {
    flex-grow: 1;
    height: 100%; /* Ocupa a altura total do layout-container */
    display: flex;
    flex-direction: column;
    padding: 1rem; /* Padding interno para o conteúdo da coluna */
    
    border-radius: 5px;
    box-shadow: 0 0 8px rgba(0, 0, 0, 0.3);
}

/* Definindo larguras para as colunas em telas maiores (simulando col-md-*) */
/* Otimizado para 1280x720px e acima */
@media (min-width: 992px) { /* Usar min-width: 992px (lg) ou 1200px (xl) para alvos de tela maiores */
    .left-column {
        flex-basis: 22%; /* Um pouco mais estreita que col-md-3, para 1280px */
        max-width: 22%;
    }
    .center-column {
        flex-basis: 56%; /* Mais larga para perícias e ações */
        max-width: 56%;
    }
    .right-column {
        flex-basis: 22%; /* Alinhada com a esquerda */
        max-width: 22%;
    }
    /* Garante que todas as colunas sejam visíveis e não "single active" */
    .column:not(.active) {
        display: flex !important; /* Sobrescreve d-none se Layout.tsx usa a classe 'active' para mobile */
    }
}
/* Para telas entre 768px e 991px (md) */
@media (min-width: 768px) and (max-width: 991.98px) {
    .left-column {
        flex-basis: 25%;
        max-width: 25%;
    }
    .center-column {
        flex-basis: 50%;
        max-width: 50%;
    }
    .right-column {
        flex-basis: 25%;
        max-width: 25%;
    }
    .column:not(.active) {
        display: flex !important;
    }
}


