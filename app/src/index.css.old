/* Configura o layout principal */
.layout-container {
  display: flex;
  
  width: 100%; /* 100% da largura da página */
}

/* Coluna esquerda */
.left-column {
  flex: 0 0 20%; /* Largura fixa de 20% */

  border-right: 1px solid #dee2e6;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 1rem;
}

/* Coluna central */
.center-column {
  flex: 0 0 60%; /* Largura fixa de 60% */

  color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;

    height: 90%; /* Garantir que ocupe a altura completa do contêiner pai */
}

/* Coluna direita */
.right-column {
  flex: 0 0 20%; /* Largura fixa de 20% */
  border-left: 1px solid #dee2e6;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start; /* Alinhar conteúdo no topo */
  padding: 1rem;
  height: 95%; /* Garantir que ocupe a altura completa do contêiner pai */
}

