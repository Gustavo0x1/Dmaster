/* Estilos para as Abas (Tabs) no ActionManager */
.nav-tabs {
    border-bottom: 1px solid #6c757d; /* Borda inferior para as abas */
}
.nav-tabs .nav-link {
    color: #adb5bd; /* Cor do texto das abas inativas */
    border: 1px solid transparent;
    border-top-left-radius: 0.25rem;
    border-top-right-radius: 0.25rem;
    margin-bottom: -1px; /* Para as bordas se sobreporem */
    background-color: transparent;
}
.nav-tabs .nav-link:hover, .nav-tabs .nav-link:focus {
    color: #ffc107; /* Cor ao passar o mouse/focar */
    border-color: transparent transparent #6c757d; /* Borda inferior no hover */
    background-color: #495057; /* Fundo sutil no hover */
}
.nav-tabs .nav-link.active {
    color: #ffc107; /* Cor da aba ativa */
    background-color: #343a40; /* Fundo da aba ativa, igual ao background do card */
    border-color: #6c757d #6c757d #343a40; /* Borda da aba ativa */
    font-weight: bold;
}
.tab-content {
    /* Garante que o conteúdo das abas ocupe o espaço restante */
    flex-grow: 1;
    display: flex; /* Para que o tab-pane show active ocupe a altura total */
}
.tab-content .tab-pane.show.active {
    flex-grow: 1;
    display: flex;
    flex-direction: column; /* Para que o card dentro preencha a altura */
}

/* Opcional: Ajustes para garantir que o texto das abas não seja muito grande */
.nav-tabs .nav-item .nav-link {
    font-size: 0.85rem;
}

